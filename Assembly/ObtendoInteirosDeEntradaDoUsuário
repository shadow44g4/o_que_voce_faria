.data 
	prompt:  .asciiz " Diga sua idade: "
	mensagem: .asciiz "\nSua idade é "
	
.text 
	#Prompt para entrada da idade do usuario 
	li $v0, 4  #print uma String (4 pq uma palavra contem 4 bits)
	la  $a0, prompt
	syscall
	
	#obter a idade do usuário
	li $v0, 5   # carrega int para leitura
	syscall
	
	#Armazenar o resultado em $t0
	move $t0,$v0
	
	#Display
	li $v0, 4
	la $a0, mensagem 
	syscall
	 
	#print para mostrar a idade
	li $v0,1
	move $a0, $t0
	syscall
	
	  