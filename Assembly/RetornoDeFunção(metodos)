.data 
	message: .asciiz "teste com uso de funcões de retorno\n não é que deu certo :)   \n"
	
.text
	main: #função principal
		jal displayMessage #retorna o link
		addi $s0, $zero,5 # add um inteiro no caso 5 + $zero(de valor 0) == $s0 =5
		
		li $v0,1 # print um inteiro (no caso o 5)
		add $a0, $zero,$s0 
		syscall
	li $v0,10 #representa o comando para sair do sistema
	syscall # chamada do sistema 
	
	
	displayMessage:#função secundaria a ser chamada
	li $v0, 4 #representa o comando para print String
	la $a0, message # chama a variavel mensagem e carregando no $a0
	syscall  # chamada do sistema 
	
	jr $ra # faz retornar a função principal
