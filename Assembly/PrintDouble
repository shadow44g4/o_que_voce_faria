.data 
	myDouble: .double 7.202
	zeroDouble: .double 0.0
	# variaveis Double  que refernciam os numeros a serem  chamados
.text
	ldc1 $f2, myDouble    # carrega a variavel Double no compressor
	ldc1 $f0, zeroDouble
	
	li $v0,3 #representa o comando para print Double
	add.d $f12,$f2,$f0 # adição de variaveis flutuantes f0+f2=f12
	syscall # chamada do sistema 
	
	
	