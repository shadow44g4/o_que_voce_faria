.data
	 novaLinha: .asciiz "\n"

.text
	# video aula no link abaixo
	#https://www.youtube.com/watch?v=3napwKvocSU&index=17&list=PL5b07qlmA3P6zUdDf-o97ddfpvPFuNa5A
	main: #metodo principal
	     addi $s0, $zero, 10 
	     
	     jal incrementarMeuRegistro
	     
	     #print um nova linha
	     li $v0, 4
	     la $a0, novaLinha
	     syscall
	    
	   jal printOValor
	   	     
	     
	#linha abaixo sinaliza o fim do programa
	li $v0, 10 
	syscall
	
	incrementarMeuRegistro:
		add $sp,$sp, -4
		sw  $s0,0($sp)
		addi $s0,$s0,30 
		
		# procedimento aninhado
		jal printOValor 
		
		lw $s0,0($sp)
		addi $sp,$sp,4
		
		jr $ra
		
		
	printOValor:
		#print um novo valor na função 
		li $v0,1
		move $a0,$s0
		syscall
		
		jr $ra
		
		
		